{
  "name": "kompakkt.repo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "prettier": "prettier 'src/**/*.ts' --write",
    "cypress": "concurrently \"ng serve\" \"cypress open\""
  },
  "contributors": [
    {
      "name": "Jan G. Wieners",
      "email": "jan.wieners@uni-koeln.de",
      "url": "https://jan-wieners.de"
    },
    {
      "name": "Kai Niebes"
    },
    {
      "name": "Zoe Schubert"
    }
  ],
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/cypress/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "parser": "typescript",
    "requirePragma": false,
    "insertPragma": false,
    "endOfLine": "lf",
    "printWidth": 80
  },
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.3",
    "@angular/cdk": "^9.0.0-rc.4",
    "@angular/common": "~9.0.0-rc.3",
    "@angular/compiler": "~9.0.0-rc.3",
    "@angular/core": "~9.0.0-rc.3",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~9.0.0-rc.3",
    "@angular/material": "^9.0.0-rc.4",
    "@angular/platform-browser": "~9.0.0-rc.3",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.3",
    "@angular/router": "~9.0.0-rc.3",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "particles.js": "^2.0.0",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.3",
    "@angular/cli": "~9.0.0-rc.3",
    "@angular/compiler-cli": "~9.0.0-rc.3",
    "@angular/language-service": "~9.0.0-rc.3",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.12",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.0.0",
    "cypress": "^3.6.1",
    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.0.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.6.4"
  }
}
